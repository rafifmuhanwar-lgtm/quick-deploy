{
    "version": 2,
    "builds": [
        {
            "src": "api/deploy.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/deploy",
            "dest": "/api/deploy.js",
            "methods": [
                "POST",
                "OPTIONS"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}